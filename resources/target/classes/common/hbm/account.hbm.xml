<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  
  <class name="com.huatai.common.account.AssetAccountEntity" schema="ACCOUNT" table="T_ASSET_ACCOUNT" polymorphism="explicit"> 
  	<composite-id>
	    <key-property 	column="INVESTOR_ACCOUNT" 			name="investorAccount" 				type="java.lang.String"/>
		<key-property	column="PRODUCT_ACCOUNT" 			name="productAccount" 				type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 				name="assetAccount" 				type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 		name="assetAccountType" 			length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
  	</composite-id>
	<property 	column="STATUS" 						name="status" 						length="30">
   		<type 	name="org.hibernate.type.EnumType">
   			<param 	name="enumClass">com.huatai.common.type.AccountStatus</param>   
       		<param 	name="type">12</param>
   		</type>
	</property>
  	<property 	column="SHARED" 			name="shared" 						type="java.lang.Boolean"/> 
  </class>
  
  <class name="com.huatai.common.account.TradingAccountEntity" schema="ACCOUNT" table="T_TRADING_ACCOUNT" polymorphism="explicit"> 
  	<composite-id>
	    <key-property 	column="INVESTOR_ACCOUNT" 			name="investorAccount" 			type="java.lang.String"/>
		<key-property	column="PRODUCT_ACCOUNT" 			name="productAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 				name="assetAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 		name="assetAccountType" 		length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="TRADING_ACCOUNT" 			name="tradingAccount" 			type="java.lang.String"/>
  	</composite-id>
	<property 	column="STATUS" 						name="status" 						length="30">
   		<type 	name="org.hibernate.type.EnumType">
   			<param 	name="enumClass">com.huatai.common.type.AccountStatus</param>   
       		<param 	name="type">12</param>
   		</type>
	</property>
  	<property 	column="CLIENT_PERMISSIONS" 	name="clientPermissions" 			type="java.lang.String"/>
	<property	column="ALLOWED_ENTRUSTS" 		name="allowedEntrusts" 				type="java.lang.String"/>
	<property	column="RESTRICTION" 			name="restriction" 					type="java.lang.String"/> 
  </class>
  
  <class name="com.huatai.common.persistence.TradingAccountToSecurityAccountMapping" schema="ACCOUNT" table="T_TRADING_ACCOUNT_TO_SECURITY_ACCOUNT" polymorphism="explicit"> 
  	<composite-id>
		<key-property 	column="TRADING_ACCOUNT" 			name="tradingAccount" 			type="java.lang.String"/>
		<key-property 	column="SECURITY_ACCOUNT" 			name="securityAccount" 			type="java.lang.String"/>
  	</composite-id>
  </class>
  
  <class name="com.huatai.common.account.SecurityAccountEntity" schema="ACCOUNT" table="T_SECURITY_ACCOUNT" polymorphism="explicit">
	<id 		column="SECURITY_ACCOUNT" 				name="securityAccount" 				type="java.lang.String"/>
	<property 	column="EXCHANGE_TYPE" 					name="exchangeType" 				length="4">
		<type 	name="org.hibernate.type.EnumType">
			<param 	name="enumClass">com.huatai.common.type.ExchangeType</param>   
			<param 	name="type">12</param>
		</type>
	</property>
    <property 	column="SECURITY_ACCOUNT_TYPE" 			name="securityAccountType" 			length="30">
   		<type 	name="org.hibernate.type.EnumType">
   			<param 	name="enumClass">com.huatai.common.type.SecurityAccountType</param>   
       		<param 	name="type">12</param>
   		</type>
	</property>
    <property 	column="MAIN_SECURITY_ACCOUNT" 			name="mainSecurityAccount" 			type="java.lang.Boolean"/>
    <property 	column="STATUS" 						name="status" 						length="30">
   		<type 	name="org.hibernate.type.EnumType">
   			<param 	name="enumClass">com.huatai.common.type.AccountStatus</param>   
       		<param 	name="type">12</param>
   		</type>
	</property>
	<property  	column="SECURITY_ACCOUNT_PERMISSION" 	name="securityAccountPermission" 	type="java.lang.String"/>
    <property 	column="SEAT_NO" 						name="seatNo" 						type="java.lang.String"/>
    <property 	column="BRANCH_NO" 						name="branchNo" 					type="java.lang.String"/>
    <property 	column="PBU" 							name="pbu" 							type="java.lang.String"/>
  </class>
  
  <class name="com.huatai.common.transaction.CommissionTemplate" schema="ACCOUNT" table="T_COMMISSION_TEMPLATE" polymorphism="explicit">
  	<composite-id>
	    <key-property 	column="INVESTOR_ACCOUNT" 			name="investorAccount" 				type="java.lang.String"/>
		<key-property	column="PRODUCT_ACCOUNT" 			name="productAccount" 				type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 				name="assetAccount" 				type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 		name="assetAccountType" 			length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="OBJECT_ID" 					name="id" 							type="java.lang.String"/>
		<key-property 	column="EXCHANGE_TYPE" 				name="exchangeType" 				length="4">
			<type 	name="org.hibernate.type.EnumType">
				<param 	name="enumClass">com.huatai.common.type.ExchangeType</param>   
				<param 	name="type">12</param>
			</type>
		</key-property>
		<key-property 	column="SECURITY_TYPE" 				name="securityType" 				length="10">
		   	<type 	name="org.hibernate.type.EnumType">
		   		<param 	name="enumClass">com.huatai.common.type.SecurityType</param>   
		       	<param 	name="type">12</param>
		   	</type>
		</key-property>
  	</composite-id>
  	<property 	column="COMMISSION_TEMPLATE_TYPE" 			name="commissionTemplateType" 		length="10">
  		<type 	name="org.hibernate.type.EnumType">
			<param 	name="enumClass">com.huatai.common.type.CommissionTemplateType</param>   
			<param 	name="type">12</param>
		</type>
  	</property>
    <property 	column="BUY_COMMISSION_RATE" 			name="buyCommissionRate" 			type="java.lang.Double"/>
    <property 	column="MIN_BUY_COMMISSION" 			name="minBuyCommission" 			type="java.lang.Double"/>
    <property 	column="SELL_COMMISSION_RATE" 			name="sellCommissionRate" 			type="java.lang.Double"/>
    <property 	column="MIN_SELL_COMMISSION" 			name="minSellCommission" 			type="java.lang.Double"/>
    <property 	column="STAMP_DUTY_RATE" 				name="stampDutyRate" 				type="java.lang.Double"/>
    <property 	column="TRANSFER_FEE_RATE" 				name="transferFeeRate" 				type="java.lang.Double"/>
    <property 	column="CREATED" 						name="created" 						type="java.sql.Timestamp"/>
    <property 	column="MODIFIED" 						name="modified" 					type="java.sql.Timestamp"/>
  </class>
  
</hibernate-mapping> 
