<?xml version="1.0" encoding="UTF-8"?>
<t2sdk-configuration>
	<performance heartbeatTime="400" acquireConnWaitTime="3000"
		registerTime="5" reconnInterval="3" callBackTime="5000" enmCompress="true" />
	<parents>
		<parent parentName="uf-001" safeLevel="none">
			<limit licenseFile="classpath:refdata/hundsun/license.dat"
				encrypt="HSBlowfish" />
			<members>
				<member no="5" address="168.168.203.21" port="9002" charset="gbk"
					poolSize="1" />
			</members>
		</parent>
	</parents>
	<callBackMethods>
		<!-- 消息中心的回调必须增加这一项 -->
		<method id="-1" className="com.hundsun.mcapi.MCT2sdkCallback" />
	</callBackMethods>

	<logAdapter className="com.hundsun.t2sdk.impl.util.CommonLogAdapter" />
	<mc parentName="uf-001" type="0" cachedir="./mc" inittimeout="3000">
		<subscriber subname="ufisub"
			subcallback="com.huatai.platform.downstream.adaptor.uf.UFInvokeCallback" timeout="3000" />
		<publisher pubname="xuxp" msgCount="200" isResetNo="false" />
		<mclog dir="./mclog" />
	</mc>

</t2sdk-configuration>