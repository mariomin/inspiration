<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.huatai.common.account.TradingAccountEntity" schema="ACCOUNT" table="T_SECURITY_ACCOUNT"> 
  	<composite-id>
	    <key-property 	column="COMPANY_ID" 			name="investorAccount" 			type="java.lang.String"/>
		<key-property	column="PRODUCT_ID" 			name="productAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 			name="assetAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 		name="assetAccountType" 			length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="TRADE_ACCOUNT" 			name="tradingAccount" 			type="java.lang.String"/>
  	</composite-id>
  </class>
</hibernate-mapping>