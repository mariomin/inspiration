<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  
  <class name="com.huatai.common.intraday.IntradayPortfolioPositionExtensionRecord" schema="EXTENSION" table="T_INTRADAY_PORTFOLIO_POSITION_EXTENSION_RECORD" polymorphism="explicit">
  	<composite-id>
	    <key-property 	column="INVESTOR_ACCOUNT" 		name="investorAccount" 			type="java.lang.String"/>
		<key-property	column="PRODUCT_ACCOUNT" 		name="productAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 			name="assetAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 	name="assetAccountType" 		length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="TRADING_ACCOUNT" 		name="tradingAccount" 			type="java.lang.String"/>
		<key-property 	column="SECURITY_ACCOUNT" 		name="securityAccount" 			type="java.lang.String"/>
		<key-property 	column="SYMBOL" 				name="symbol" 					type="java.lang.String"/>
		<key-property 	column="PORTFOLIO_NO" 			name="portfolioNo" 				type="java.lang.String"/>
  	</composite-id>
	<property 	column="CUM_BUY_FREEZED_QTY" 			name="cumBuyFreezedQty" 		type="java.lang.Double"/>
    <property 	column="CREATED" 						name="created" 					type="java.sql.Timestamp"/>
    <property 	column="MODIFIED" 						name="modified" 				type="java.sql.Timestamp"/>
  </class>
  
  <class name="com.huatai.common.intraday.IntradayPortfolioItem" schema="EXTENSION" table="T_INTRADAY_PORTFOLIO_ITEM" polymorphism="explicit">
  	<composite-id>
	    <key-property 	column="INVESTOR_ACCOUNT" 		name="investorAccount" 			type="java.lang.String"/>
		<key-property	column="PRODUCT_ACCOUNT" 		name="productAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT" 			name="assetAccount" 			type="java.lang.String"/>
		<key-property 	column="ASSET_ACCOUNT_TYPE" 	name="assetAccountType" 		length="20">
			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.AssetAccountType</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="TRADING_ACCOUNT" 		name="tradingAccount" 			type="java.lang.String"/>
		<key-property 	column="SECURITY_ACCOUNT" 		name="securityAccount" 			type="java.lang.String"/>
		<key-property 	column="SYMBOL" 				name="symbol" 					type="java.lang.String"/>
		<key-property 	column="ORDER_SIDE" 			name="orderSide" 				length="10">
   			<type 	name="org.hibernate.type.EnumType">
   				<param 	name="enumClass">com.huatai.common.type.OrderSide</param>   
       			<param 	name="type">12</param>
   			</type>
		</key-property>
		<key-property 	column="PORTFOLIO_NO" 			name="portfolioNo" 				type="java.lang.String"/>
  	</composite-id>
	<property 	column="PORTFOLIO_NAME" 				name="portfolioName" 			type="java.lang.String"/>
	<property 	column="PORTFOLIO_TYPE" 				name="portfolioType" 			type="java.lang.String"/>
	<property 	column="POSITION_LIMIT" 				name="positionLimit" 			type="java.lang.Double"/>
    <property 	column="CREATED" 						name="created" 					type="java.sql.Timestamp"/>
    <property 	column="MODIFIED" 						name="modified" 				type="java.sql.Timestamp"/>
  </class>
  
</hibernate-mapping> 
