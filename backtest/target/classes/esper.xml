<?xml version="1.0" encoding="UTF-8"?><!-- 	Defaullt (empty) configuration file.--><esper-configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    xmlns="http://www.espertech.com/schema/esper"    xsi:noNamespaceSchemaLocation="esper-configuration-5-0.xsd">    	<event-type name="Quote" class="com.huatai.common.marketdata.Quote"/>		<event-type name="Trade" class="com.huatai.common.marketdata.Trade"/> 		<plugin-singlerow-function name="formalizeTime" function-class="com.huatai.strategy.util.EPLFunctionUtil" function-method="formalizeTime" />		<plugin-singlerow-function name="calMidPrice" function-class="com.huatai.strategy.util.EPLFunctionUtil" function-method="calMidPrice" />			<plugin-singlerow-function name="isValidTime" function-class="com.huatai.strategy.util.EPLFunctionUtil" function-method="isValidTime"/>	<plugin-singlerow-function name="isMarketAtClose" function-class="com.huatai.strategy.util.EPLFunctionUtil" function-method="isMarketAtClose"/>		<engine-settings>		<defaults>			<threading>				<internal-timer enabled="false" msec-resolution="100"/>			</threading>		</defaults>	</engine-settings>	</esper-configuration>